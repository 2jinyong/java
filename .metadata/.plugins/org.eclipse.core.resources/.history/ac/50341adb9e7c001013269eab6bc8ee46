package sec06;

public class KoreanExam {

	public static void main(String[] args) {

		Korean k1 = new Korean("123456-1234567", "홍길동");
		
		System.out.println(k1.nation);
		System.out.println(k1.name);
		System.out.println(k1.ssn);
		
		//final값은 변경 불가 
		k1.nation = "미국";
		k1.ssn = "234567-2345678";
		k1.name = "김순이";
		
		System.out.println(k1.nation);
		System.out.println(k1.name);
		System.out.println(k1.ssn);

	}

}
